<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:deployApplicationRequest  xmlns:ns2="module.user.wsdl.fisheries.ec.europa.eu">
    <method>DEPLOY_APPLICATION</method>
    <application>
        <name>Subscription</name>
        <description>Deployment descriptor of module "Subscription" version "@application.version@".</description>
        <parent>Union VMS</parent>

        <feature>
            <name>VIEW_SUBSCRIPTION</name>
            <description>Allows user to see the Subscriptions tab</description>
            <group>Subscription</group>
        </feature>
        <feature>
            <name>MANAGE_SUBSCRIPTION</name>
            <description>Allows user to create and delete and update Subscriptions</description>
            <group>Subscription</group>
        </feature>

    </application>
</ns2:deployApplicationRequest>