<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <changeSet id="UNIONVMS-4466-subscriptionexecution" author="gmanifavas">
        <createTable tableName="subscription_execution">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="pk_subscription_execution" />
            </column>
            <column name="requested_time" type="TIMESTAMP WITHOUT TIME ZONE">
                <constraints nullable="false" />
            </column>
            <column name="queued_time" type="TIMESTAMP WITHOUT TIME ZONE" />
            <column name="execution_time" type="TIMESTAMP WITHOUT TIME ZONE" />
            <column name="creation_date" type="TIMESTAMP WITHOUT TIME ZONE">
                <constraints nullable="false" />
            </column>
            <column name="status" type="VARCHAR(12)"/>
            <column name="triggered_subscription_id" type="BIGINT">
                <constraints nullable="false" />
            </column>
        </createTable>

        <addForeignKeyConstraint baseTableName="subscription_execution" baseColumnNames="triggered_subscription_id"
                                 referencedTableName="triggered_subscription" referencedColumnNames="id"
                                 constraintName="fk_subscription_execution_triggered_subscription" />
    </changeSet>
</databaseChangeLog>
